{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CustomConfiguration": {
    "PageSize": 10
  },
  "ConnectionStrings": {
    "eCommerceDb": "Server=.; Database=eCommerceDb; Trusted_Connection=true",
    "Redis": "localhost:6379"
  },
  "RedisCache": {
    "endpointCacheTimePerDay": 1,
    "shortTimeCachingByMin": 5
  }
}
